<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaofei.app2">

    <application android:allowBackup="true" android:icon="@mipmap/icon"
        android:label="@string/app_name" android:roundIcon="@mipmap/icon"
        android:supportsRtl="true" android:theme="@style/AppTheme"
        android:name="com.gaofei.app2.App2Application">
        <activity android:name="com.gaofei.app2.Test2Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".EasyService" android:process=".remoteService" android:exported="true" android:enabled="true">
            <intent-filter>
            <action android:name="com.gaofei.app2.EasyService" />
            </intent-filter>
        </service>
    </application>

</manifest>