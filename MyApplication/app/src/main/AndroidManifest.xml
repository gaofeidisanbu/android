        <activity android:name="com.gaofei.app.act.DataBindingAct" android:exported="false"/>
        <activity android:name="com.gaofei.app.act.TaskActivity" android:exported="false"/>
        <activity android:name="com.gaofei.app.act.AnrAct" android:exported="false"/>
